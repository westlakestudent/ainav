{
  "version": 2,
  "routes": [
    {
      "src": "/data/(.*)",
      "dest": "/src/data/$1"
    },
    {
      "src": "/src/data/(.*)",
      "dest": "/src/data/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
} 